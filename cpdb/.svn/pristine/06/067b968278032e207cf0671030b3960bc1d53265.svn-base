body {
	font-size: 1em;
}

#header {
	width: 100%;
	height: 100px;
	border: 1px solid;
}

#center {
	width: 100%;
	height: 100px;
	border: 1px solid;
}

#footer {
	width: 100%;
	height: 100px;
	border: 1px solid;
}

#left_container {
	width: 20%;
	border: 1px solid;
	float: left;
}

#center_container {
	width: 78%;
	border: 1px solid;
	height: 500px;
	float: left;
}

#cat_container {
	background-color: #000000;
}

.border-none {
	border: none !important;
}

.ui-menu ui-menubar ui-widget ui-widget-content ui-corner-all ui-helper-clearfix
	{
	background: #006600;
}

.menubar_background {
	background: #2E8A2E !important;
}

.ui-menuitem-text {
	color: #FFFFFF !important;
	font-weight: lighter;
	font-size: 14px;
}

.ui-state-hover, .ui-widget-content .ui-state-hover, .ui-widget-header .ui-state-hover,
	.ui-state-focus, .ui-widget-content .ui-state-focus, .ui-widget-header .ui-state-focus
	{
	/* background:#00A352 !important;
	border:1px solid !important;
	border-color:#00A352 !important;  */
	
}

.menu_sub_item {
	color: #FFFFFF !important;
	font-size: 14px;
}

.ui-menu .ui-menu-parent .ui-menu-child {
	background: #2E8A2E;
	border: none;
}

.ui-menuitem .ui-widget .ui-corner-all {
	
}

.ui-menuitem-link .ui-corner-all {
	
}

.sub_tab_container {
	
}

.ui-widget-content {
	border: 0px solid;
}

.ui-state-hover {
	background: #00B85C !important;
	border: 1px solid !important;
	border-color: #00B85C !important;
}

.ui-widget-header .ui-state-hover {
	background: none !important;
	border: 1px solid !important;
	border-color: none !important;
}

.ui-state-focus {
	background: green !important;
	border: 1px solid !important;
	border-color: green !important;
}

.ui-widget-content .ui-state-focus {
	background: none !important;
	border: 1px solid !important;
	border-color: none !important;
}

.ui-widget-header .ui-state-focus {
	background: none !important;
	border: 1px solid !important;
	border-color: none !important;
}

div.menu_bar_container a {
	color: black;
	text-decoration: none;
	font-size: 0.8em;
	margin-left: 10px;
	font-weight: bold;
}

div.menu_bar_container ul.ui-menu-list img {
	margin-left: 400px;
}

.ui-selectonemenu {
	width: 100% !important;
}

.ui-selectonemenu-label {
	width: 100% !important;
}

div.ui-layout-resizer-west {
	background-color: green;
}

.ui-tabs .ui-tabs-nav {
	background: rgba(227, 227, 227, 0);
}

.ui-state-default, .ui-widget-content .ui-state-default,
	.ui-widget-header .ui-state-default {
	background: #FFFFFF;
}

.ui-widget-header {
	border: 1px solid #FFFFFF;
}

ul.ui-tabs-nav.ui-helper-reset.ui-helper-clearfix.ui-widget-header.ui-corner-all li
	{
	border-color: white !important;
}

.ui-widget-content a {
	color: white;
}

.ui-shadow {
	box-shadow: 0px 0px 0px rgba(0, 0, 0, 0.8);
}

select, input[type="text"] {
	width: 100%;
}

input[type="password"] {
	width: 100%;
}

.ui-state-highlight, .ui-widget-content .ui-state-highlight,
	.ui-widget-header .ui-state-highlight {
	border: 1px solid #000000;
	background: #B3FFEE url("images/ui-bg_glass_55_fbf9ee_1x400.png") 50%
		50% repeat-x;
}

ul.ui-menu-list.ui-helper-reset {
	margin-top: 5px;
}