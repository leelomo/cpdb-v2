body {
	font-size: 0.75em;
	margin: 0;
	padding: 0;
	background: url("bg_2.png") center center fixed no-repeat;
	-moz-background-size: cover;
	background-size: cover;
	font-weight: normal;
	min-width: 1024px;
	font-family: Arial;
}

#index_container {
	border: 1px solid;
	min-height: 800px;
}

.container {
	width: 100%;
	float: left;
}

.main {
	width: 100%;
	float: left;
}

#header {
	width: 100%;
	height: 100px;
	border: 1px solid;
}

#center {
	width: 100%;
	height: 100px;
	border: 1px solid;
}

#footer {
	width: 100%;
	height: 100px;
	border: 1px solid;
}

#left_container {
	width: 20%;
	border: 1px solid;
	float: left;
}

#center_container {
	width: 78%;
	border: 1px solid;
	height: 500px;
	float: left;
}

.border-none {
	border: none !important;
}

/* Login box style */
.loginbox_container {
	border-collapse: collapse;
	border: none;
	background: #FFFFFF;
	font-family: Arial;
}

.login_title {
	background: #2E8A2E;
	width: 100%;
	padding: 5px 100px 5px 100px;
	text-align: center;
}

.login_form {
	margin-left: auto;
	margin-right: auto;
	margin-top: 25px;
	margin-bottom: 25px;
}

/* Menu bar style */
.menubar_background {
	background: #2E8A2E !important;
}

.ui-menuitem-text {
	color: #FFFFFF !important;
	font-weight: lighter !important;
	font-size: 14px !important;
}

.menu_sub_item {
	color: #FFFFFF !important;
	font-size: 14px;
}

.ui-menu .ui-menu-parent .ui-menu-child {
	background: #2E8A2E;
	border: none;
}

/* .ui-menubar .ui-menuitem-link{
	background:#33D685 !important;
} */
.ui-widget-content {
	border: 0px solid;
}

.ui-state-hover {
	background: #d1d1d1 !important;
	border: 1px solid !important;
	border-color: #d1d1d1 !important;
}

/* .custom_menuitem:hover{
	background:#19A347 !important;
	border:1px solid #19A347 !important;
} */
.ui-widget-header .ui-state-hover {
	background: none !important;
	border: 1px solid !important;
	border-color: none !important;
}

.ui-state-focus {
	border: 1px solid !important;
	border-color: none !important;
}

.ui-widget-content .ui-state-focus {
	border: 1px solid !important;
	border-color: none !important;
}

.ui-widget-header .ui-state-focus {
	background: none !important;
	border: 1px solid !important;
	border-color: none !important;
}

.ui-state-highlight {
	background: none !important;
}

.ui-widget-content .ui-state-highlight {
	background: #CBF5BC !important;
}

.ui-widget-header .ui-state-highlight {
	background: none !important;
}

div.menu_bar_container a {
	color: black;
	text-decoration: none;
	font-size: 0.8em;
	margin-left: 0px;
}

div.menu_bar_container ul.ui-menu-list img {
	/*  margin-left: 400px; */
	
}

#logo.div {
	width: 100%;
}

.ui-selectonemenu {
	width: 100% !important;
}

.ui-selectonemenu-label {
	width: 100% !important;
}

div.ui-layout-resizer-west {
	background-color: #D4D4D4;
	width: 2px !important;
}

.ui-tabs .ui-tabs-nav {
	background: rgba(227, 227, 227, 0);
}

.ui-state-default {
	background: none;
}

.ui-widget-content .ui-state-default {
	background: #EFEFEF;
}

.ui-widget-header .ui-state-default {
	background: none;
}

.ui-widget-header {
	border-top: none;
	border-bottom: 1px solid;
	border-left: none;
	border-right: none;
}

ul.ui-tabs-nav.ui-helper-reset.ui-helper-clearfix.ui-widget-header.ui-corner-all li
	{
	border: none;
}

.ui-widget-content a {
	color: white;
}

.ui-shadow {
	box-shadow: 0px 0px 0px rgba(0, 0, 0, 0.8);
}

/* select, input[type="text"] {
	width: 100%;
}

input[type="password"] {
	width: 100%;
} */
.ui-state-highlight, .ui-widget-content .ui-state-highlight,
	.ui-widget-header .ui-state-highlight {
	border: 1px solid #000000;
	background: #B3FFEE url("images/ui-bg_glass_55_fbf9ee_1x400.png") 50%
		50% repeat-x;
}

ul.ui-menu-list.ui-helper-reset {
	margin-top: 5px;
}

.btnUserLogout {
	border-top: none;
	border-top-left-radius: 10px;
	border-top-right-radius: 10px;
	border-bottom-left-radius: 10px;
	border-bottom-right-radius: 10px;
}

.ui-widget-content {
	background: transparent;
}

tr.ui-widget-content.ui-datatable-odd, tr.ui-widget-content.ui-datatable-even
	{
	border-color: #CFCFCF;
}

/* .ui-state-default, .ui-widget-content .ui-state-default,
	.ui-widget-header .ui-state-default {
	border-color: rgb(153, 153, 153) !important;
} */
.ui-state-default {
	border-color: none;
}

.ui-widget-content .ui-state-default {
	border-color: #B2B2B2;
}

.ui-widget-content {
	top: -5px;
}

.centeredMenuBar.ui-menubar {
	text-align: center;
}

.centeredMenuBar .ui-menu-list {
	display: inline-block;
}

.ui-grid-row.background-color-green {
	background-color: #2E8A2E;
}

/* Custom Tab css*/
.ui-state-active a, .ui-state-active a:link, .ui-state-active a:visited
	{
	font-size: 1.5em;
}

.ui-state-default a, .ui-state-default a:link, .ui-state-default a:visited
	{
	font-size: 1.5em;
}

.ui-widget-header .ui-state-default {
	border: none !important;
}

/*  Custom DataTable css */
.ui-datatable .ui-datatable-data tr.ui-state-hover {
	background: #CBF5BC !important;
	border-color: #CFCFCF !important;
}

/* browse article details css*/
input[type="submit"]:hover {
	/* 	 background:;  */
	/* color: white !important; */
	
}

/* custom button style */
.h_green_button {
	padding: 8px 90px 8px 18px;
	background: #73cc77;
	color: white;
	border: none;
}

.h_green_button:hover {
	background: #6cb26d !important;
}

.h_red_button {
	padding: 8px 90px 8px 18px;
	background: #ff5252;
	color: white;
	border: none;
}

.h_red_button:hover {
	background: #da4c4d !important;
}

.p_green_button {
	padding: 3px 90px 3px 8px !important;
	background: #73cc77 !important;
	color: white !important;
	border: none !important;
}

.p_green_button:hover {
	background: #6cb26d !important;
}

.p_red_button {
	padding: 3px 90px 3px 8px !important;
	background: #ff5252 !important;
	color: white !important;
	border: none !important;
}

.p_red_button:hover {
	background: #da4c4d !important;
}

.p_default_button:hover {
	background: #D1D1D1 !important;
	border: 1px solid #D1D1D1 !important;
}

/* custom p fieldset style */
.custom_fieldset {
	border: 1px solid #B2B2B2 !important;
	display: inline-block;
}

.menuIcon {
	background: url("menu_btn.png") no-repeat !important;
	width: 30px;
	height: 22px;
}

span.ui-icon.ui-icon-triangle-1-s {
	background: none;
}

a.ui-menuitem-link.ui-corner-all.pageName {
	background: #2E8A2E !important;
	border: 1px solid #2E8A2E !important;
}

.ui-menu  .ui-menuitem .ui-state-hover {
	background: #30AC59 !important;
	border: 1px solid #30AC59 !important;
}
